{
  "extends": "@ijlee2-frontend-configs/typescript/ember",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "embroider-css-modules-vite-app/tests/*": [
        "tests/*"
      ],
      "embroider-css-modules-vite-app/*": [
        "app/*"
      ],
      "*": [
        "types/*"
      ]
    },
    "skipLibCheck": true,
    "types": [
      "ember-source/types"
    ]
  },
  "glint": {
    "environment": [
      "ember-loose",
      "ember-template-imports"
    ]
  },
  "include": [
    "app/**/*",
    "tests/**/*",
    "types/**/*"
  ]
}
